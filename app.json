{
  "expo": {
    "name": "GoNext",
    "slug": "gonext",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "gonext",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/backgrounds/gonext-bg.png",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gonext.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/backgrounds/gonext-bg.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gonext.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "faeea4de-981b-4c48-86bc-7e4b41e073f2"
      }
    }
  }
}
